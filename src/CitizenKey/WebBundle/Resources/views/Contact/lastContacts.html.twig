{# Component displaying last created contacts #}

{% if lastContacts %}
<ul>
    {% for contact in lastContacts %}
    <li>
        <a href="{{path('app_contact', { 'contactID': contact.id })}}">
            {{contact.name}}
        </a>
    </li>
    {% endfor %}
    <li>
        <a href="{{path('app_contact_new')}}">
            {{'contacts.actions.new'|trans}}
        </a>
    </li>
</ul>
{% endif %}
