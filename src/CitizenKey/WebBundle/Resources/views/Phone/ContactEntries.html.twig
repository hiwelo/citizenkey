{# Component displaying all phone entries for a contact #}

<h3>{{'phone-numbers.title'|trans}}</h3>
<ul>
    {% if phones %}
        {% for phone in phones %}
        <li>
            <a href="tel:{{phone.number|phone_number_format('E164')}}">
                {{phone.diallableNumber}}
            </a>
        </li>
        {% endfor %}
    {% endif %}
    <li>
        <a href="{{path('app_phone_new', {'contact': contact.id})}}">{{'phone-numbers.actions.new'|trans}}</a>
    </li>
</ul>
